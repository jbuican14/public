{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "defaultPalette": false,
      "palette": [
        {"slug": "d-white", "color": "rgb(255,255,255)", "name": "demo white"},
        {
          "slug": "u-gray-100",
          "color": "rgb(243 244 246)",
          "name": "Demo Gray 100"
        },
        {
          "slug": "d-gray-200",
          "color": "rgb(229 231 235)",
          "name": "Demo Gray 200"
        },
        {
          "slug": "d-gray-300",
          "color": "rgb(209 213 219)",
          "name": "Demo Gray 300"
        },
        {
          "slug": "d-gray-400",
          "color": "rgb(156 163 175)",
          "name": "Demo Gray 400"
        },
        {
          "slug": "d-gray-500",
          "color": "rgb(107 114 128)",
          "name": "Demo Gray 500"
        },
        {
          "slug": "d-gray-600",
          "color": "rgb(75 85 99)",
          "name": "Demo Gray 600"
        },
        {
          "slug": "d-gray-700",
          "color": "rgb(55 65 81)",
          "name": "Demo Gray 700"
        },
        {
          "slug": "d-gray-800",
          "color": "rgb(31 41 55)",
          "name": "Demo Gray 800"
        },
        {
          "slug": "d-gray-900",
          "color": "rgb(17 24 39)",
          "name": "Demo Gray 900"
        },
        {
          "slug": "d-primary",
          "color": "rgb(239 68 68)",
          "name": "Demo Primary"
        },
        {"slug": "d-accent", "color": "rgb(67 56 202)", "name": "Demo Accent"},
        {
          "slug": "d-success",
          "color": "rgb(52 211 153)",
          "name": "Demo Success"
        },
        {"slug": "d-info", "color": "rgb(96 165 250)", "name": "Demo Info"},
        {
          "slug": "d-warning",
          "color": "rgb(251 146 60)",
          "name": "Demo Warning"
        },
        {"slug": "d-danger", "color": "rgb(236 72 153)", "name": "Demo Danger"}
      ],
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "defaultGradients": false,
      "duotone": [
        {
          "colors": ["#000", "#FFF"],
          "slug": "black-and-white",
          "name": "Black and White"
        }
      ]
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            {"slug": "d-primary", "color": "#ef4444", "name": "Demo Primary"}
          ],
          "link": true,
          "custom": true
        },
        "typography": {
          "fontFamilies": [
            {
              "fontFamily": "Pecifico, sans=serif",
              "slug": "d-pecifico",
              "name": "demo Pecifico"
            }
          ]
        }
      },
      "core/cover": {
        "color": {
          "duotone": [],
          "customDuotone": false
        }
      },
      "core/preformatted": {
        "typography": {
          "fontSizes": [],
          "customFontSize": false
        }
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Rubik, sans=serif",
          "slug": "d-rubik",
          "name": "demo Rubik"
        }
      ],
      "fontSizes": [
        {"slug": "small", "size": "0.75rem", "name": "small"},
        {"slug": "medium", "size": "1.25rem", "name": "Medium"},
        {"slug": "large", "size": "2.25rem", "name": "Large"},
        {"slug": "x-large", "size": "3rem", "name": "Extra large"},
        {"slug": "gigantic", "size": "3.75rem", "name": "Gigantic"}
      ],
      "lineHeight": true,
      "dropCap": true,
      "fontWeight": true,
      "fontStyle": true,
      "textTransform": true,
      "letterSpacing": true,
      "textDecoration": true
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--d-gray-700)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--d-primary)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "color": {
          "gradient": "var(--wp--preset--gradient--d-summer-demo)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "2px",
          "radius": "10px",
          "style": "solid",
          "color": "var(--wp--preset--color--d-primary)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--u-pacifico)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--d-rubik)",
      "fontSize": "16px",
      "fontStyle": "normal",
      "fontWeight": "normal",
      "lineHeight": "inherit",
      "textDecoration": "none",
      "textTransform": "none"
    }
  }
}
